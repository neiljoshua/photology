-if signed_in?
	.photo-links
		= link_to 'Back', user_path(current_user.id), class: 'photo-links__link'
		= link_to 'Edit', editphotos_path(user_id: current_user.id), "data-turbolinks-action" => "replace", class: 'photo-links__link'
		= link_to 'Remove', [@photo.user, @photo], method: :delete, data: { confirm: 'Are you sure?'}, class: 'photo-links__link'
= render "shared/photo"
